{
    "name": "Dark Mode for Google G Suite",
    "description": "Dark mode for Google Docs, Google Slides, Google Sheets, Google Drawings and more! Support coming soon for other Google services!",
    "version": "1.3",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["https://www.google.com/*","https://docs.google.com/*","https://drive.google.com/*", "https://classroom.google.com/*", "https://mail.google.com/*", "https://calendar.google.com/*", "https://meet.google.com/*", "https://myaccount.google.com/*", "https://contacts.google.com/*", "https://mail.google.com/chat/*", "https://chat.google.com/*"], 
        "exclude_matches":["https://mail.google.com/chat/"],
        "css": ["google.css", "chat.css", "drive.css", "docs.css", "classroom.css", "gmail.css", "forms.css", "calendar.css", "meet.css", "sites.css", "myaccount.css", "contacts.css"]
      }
    ]
  }