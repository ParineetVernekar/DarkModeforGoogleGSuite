{
    "name": "Dark Mode for Google G Suite",
    "description": "Dark mode for Google Docs, Google Slides, Google Sheets, Google Drawings and more! Support coming soon for other Google services!",
    "version": "1.3",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["https://www.google.com/*","https://docs.google.com/*","https://drive.google.com/*", "https://chat.google.com/*", "https://classroom.google.com/*", "https://mail.google.com/*"], 
        "css": ["google.css", "chat.css", "drive.css", "docs.css", "classroom.css", "gmail.css", "forms.css"]
      }
    ]
  }